{
	"name": "Reverse Light",
	"author": "Jande Masmo",
    "maintainers": [
        "Jande Masmo <github.com/jandemasmo>",
    ],
	"colors": {
		"editor.background": "#f7f7f7",
		"editor.foreground": "#8e0090",
		"editor.selectionBackground": "#b0c5e3",
		"editor.findMatchBackground": "#b0c5e3cc",
		"editor.findMatchHighlightBackground": "#ffb442cc",
		"editor.lineHighlightBackground": "#f7f7f7",
		"editorCursor.foreground": "#8e0090",
		"editorWhitespace.foreground": "#ffffff",
		"editor.wordHighlightBackground": "#eceba3cc",
		"peekViewResult.matchHighlightBackground": "#ffb442cc",
		"editorIndentGuide.background": "#727272",
		"editorLineNumber.foreground": "#727272",
        "statusBar.background": "#8e0090",
		"activityBarBadge.background": "#8e0090",
		"activityBar.background": "#3f3f3f",
		"sideBar.border": "#cccccc"
	},
	"tokenColors": [
		{
			"scope": ["emphasis"],
			"settings": {
			  "fontStyle": "italic"
			}
		  },
		  {
			"scope": ["strong"],
			"settings": {
			  "fontStyle": "bold"
			}
		  },
		  {
			"scope": ["header"],
			"settings": {
			  "foreground": "#00c1be"
			}
		  },
		  {
			"scope": ["source"],
			"settings": {
			  "foreground": "#727272"
			}
		  },
		  {
			"scope": ["meta.diff", "meta.diff.header"],
			"settings": {
			  "foreground": "#5A4B81"
			}
		  },
		  {
			"scope": ["markup.inserted"],
			"settings": {
			  "foreground": "#07bc3a"
			}
		  },
		  {
			"scope": ["markup.deleted"],
			"settings": {
			  "foreground": "#E96379"
			}
		  },
		  {
			"scope": ["markup.changed"],
			"settings": {
			  "foreground": "#E89E64"
			}
		  },
		  {
			"scope": ["invalid"],
			"settings": {
			  "foreground": "#E96379",
			  "fontStyle": "underline italic"
			}
		  },
		  {
			"scope": ["invalid.deprecated"],
			"settings": {
			  "foreground": "#000000",
			  "fontStyle": "underline italic"
			}
		  },
		  {
			"scope": ["entity.name.filename"],
			"settings": {
			  "foreground": "#ffd800"
			}
		  },
		  {
			"scope": ["markup.error"],
			"settings": {
			  "foreground": "#E96379"
			}
		  },
		  {
			"name": "Underlined markup",
			"scope": ["markup.underline"],
			"settings": {
			  "fontStyle": "underline"
			}
		  },
		  {
			"name": "Bold markup",
			"scope": ["markup.bold"],
			"settings": {
			  "fontStyle": "bold",
			  "foreground": "#E89E64"
			}
		  },
		  {
			"name": "Markup headings",
			"scope": ["markup.heading"],
			"settings": {
			  "fontStyle": "bold",
			  "foreground": "#00c1be"
			}
		  },
		  {
			"name": "Markup italic",
			"scope": ["markup.italic"],
			"settings": {
			  "foreground": "#ffd800",
			  "fontStyle": "italic"
			}
		  },
		  {
			"name": "Bullets, lists (prose)",
			"scope": [
			  "beginning.punctuation.definition.list.markdown",
			  "beginning.punctuation.definition.quote.markdown",
			  "punctuation.definition.link.restructuredtext"
			],
			"settings": {
			  "foreground": "#988bc7"
			}
		  },
		  {
			"name": "Inline code (prose)",
			"scope": ["markup.inline.raw", "markup.raw.restructuredtext"],
			"settings": {
			  "foreground": "#07bc3a"
			}
		  },
		  {
			"name": "Links (prose)",
			"scope": ["markup.underline.link", "markup.underline.link.image"],
			"settings": {
			  "foreground": "#988bc7"
			}
		  },
		  {
			"name": "Link text, image alt text (prose)",
